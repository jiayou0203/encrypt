from random import Random
import hashlib

def  create_md5(pwd,salt):
    md5_obj=hashlib.md5()
    md5_obj.update((salt+pwd).encode("utf-8"))
    return md5_obj.hexdigest()


pwd="abcd@1234"
salt="TRANSSNET_SALT_MD5_KEY"
md5=create_md5(pwd,salt)


print('[pwd]\n',pwd )
print('[salt]\n', salt)
print('[md5]\n', md5)
